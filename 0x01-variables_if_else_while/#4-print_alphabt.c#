#include <stdio.h>

/**
 * main - entry point
 *
 * Return: Always 0
 */
int main(void)

{
char x = 'a';
while (x <= 'z')
{
	
	if (x != 'q' && x != 'e')
		putchar(x);
}
return (0);
}
(setq c-default-style "bsd"
       c-basic-offset 8
       tab-width 8
 indent-tabs-mode t)

(require 'whitespace)
	(setq whitespace-style '(face empty lines-tail trailing))
	(global-whitespace-mode t)

	(setq column-number-mode t)

	(setq make-backup-files nil) ; stop creating ~ files